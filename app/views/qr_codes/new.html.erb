<% provide(:title, "QR Code") %>
<%= form_for(@qr) do |f| %>
  <%= render 'shared/qr_code_error_messages' %>
          
  <%= f.label :location %>
  <%= f.text_field :location, class: 'form_control' %>
  <br>

  <%= f.label :content %>
  <%= f.text_field :content, class: 'form_control' %>
  <br>
            
  <%= f.submit "Create QR Code", class: "btn btn-primary" %>
<% end %>