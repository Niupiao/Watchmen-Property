<%= form_for(@qr) do |f| %>
  <%= render 'shared/qr_code_error_messages' %>
          
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form_control' %>
  <br>

  <%= f.label :body %>
  <%= f.text_field :body, class: 'form_control' %>
  <br>
            
  <%= f.submit "Create QR Code", class: "btn btn-primary" %>
<% end %>