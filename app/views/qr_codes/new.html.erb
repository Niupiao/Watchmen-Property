<% provide(:title, "QR Code") %>
<%= form_for @qr, :url => qr_codes_path(property: params[:property]), :action => 'POST' do |f| %>
  <%= render 'shared/qr_code_error_messages' %>
          

                   <header class="panel-heading">
                            <h3>New QR Code</h3>
                         </header>
                     <section class="panel">
                     
                         <div class="panel-body">
                             <div class="form">
                                 <form class="form-validate form-horizontal " id="register_form" method="get" action="">
                                     <div class="form-group ">
                                         <%= f.label :location, class: "control-label col-lg-2"%><span class="required"></span>
                                         <div class="col-lg-10">
                                             <%= f.text_field :location, class: 'form-control' %>
                                         </div>
                                     </div>
                                     <div class="form-group ">
                                         <%= f.label :content, class: "control-label col-lg-2"%><span class="required"></span>
                                         <div class="col-lg-10">
                                              <%= f.text_field :content, class: 'form-control' %>
                                         </div>
                                     </div>
                                     <div class="form-group">
                                         <div class="col-lg-offset-2 col-lg-10">
                                             <!--<button class="btn btn-primary" type="submit">Create Employee</button>-->
                                             <%= f.submit "Create QR Code", class: "btn btn-primary" %>
                                             <%= render 'shared/qr_code_error_messages' %>
                                             <% end %>
                                         </div>
                                     </div>
                                 </form>
                             </div>
                         </div>
                     </section>
        